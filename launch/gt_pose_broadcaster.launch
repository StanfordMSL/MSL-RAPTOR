<launch>
  <arg name="id"                  default="7" />
  <arg name="robot_type"          default="quad" />
  <arg name="ns"                  value="/$(arg robot_type)$(arg id)" />
  <arg name="gt_poses_to_broadcast_yaml" default="gt_poses_to_broadcase.yaml" />

  <!-- Launch our main node -->
  <node name="MSL_RAPTOR" pkg="msl_raptor" type="gt_pose_broadcaster.py" output="screen">
    <param name="ns"                         value="$(arg ns)"/> 
    <param name="gt_poses_to_broadcast_yaml" value="gt_poses_to_broadcase.yaml" />
  </node>

</launch>
